{
  "name": "typecal",
  "version": "0.0.1",
  "description": "Yet another iCalendar parser & generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "debug": "node --inspect=5858 -r ts-node/register src/index.ts",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "test": "jest ./src --watch",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint src/**/*.ts --fix",
    "start:prod": "nodemon src/index.ts"
  },
  "keywords": [
    "ical",
    "icalendar",
    "typescript",
    "ts",
    "microsoft",
    "outlook",
    "calendar",
    "time",
    "gmail",
    "rfc5455"
  ],
  "author": "Jackson Ming Hu <huming2207@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huming2207/TypeCal"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/omit-empty": "^1.0.0",
    "axios": "^0.21.0",
    "cerialize": "^0.1.18",
    "luxon": "^1.25.0",
    "moment": "^2.29.1",
    "omit-empty": "^1.0.0",
    "query-string": "^6.13.6",
    "rrule": "^2.6.6",
    "ts-enum-util": "^4.0.2",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3",
    "typestate": "^1.0.6"
  },
  "devDependencies": {
    "@babel/polyfill": "7.12.1",
    "@types/luxon": "1.25.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.6.1",
    "nodemon": "2.0.6",
    "prettier": "2.1.2",
    "ts-node": "9.1.1"
  },
  "files": [
    "dist/**/*"
  ]
}

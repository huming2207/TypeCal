{
  "name": "typecal",
  "version": "0.0.1",
  "description": "Yet another iCalendar parser & generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "debug": "node --inspect=5858 -r ts-node/register src/index.ts",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "test": "jest ./src --watch",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint src/**/*.ts --fix",
    "start:prod": "nodemon src/index.ts"
  },
  "keywords": [
    "ical",
    "icalendar",
    "typescript",
    "ts",
    "microsoft",
    "outlook",
    "calendar",
    "time",
    "gmail",
    "rfc5455"
  ],
  "author": "Jackson Ming Hu <huming2207@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huming2207/TypeCal"
  },
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/omit-empty": "^1.0.0",
    "axios": "^0.20.0",
    "cerialize": "^0.1.18",
    "luxon": "^1.24.1",
    "moment": "^2.26.0",
    "omit-empty": "^1.0.0",
    "query-string": "^6.13.1",
    "rrule": "^2.6.4",
    "ts-enum-util": "^4.0.2",
    "ts-jest": "^26.1.0",
    "typescript": "^4.0.0",
    "typestate": "^1.0.6"
  },
  "devDependencies": {
    "@babel/polyfill": "7.10.4",
    "@types/luxon": "1.24.3",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.4.2",
    "nodemon": "2.0.4",
    "prettier": "2.1.0",
    "ts-node": "9.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}

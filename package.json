{
  "name": "typecal",
  "version": "0.0.1",
  "description": "Yet another iCalendar parser & generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "debug": "node --inspect=5858 -r ts-node/register src/index.ts",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "test": "jest ./src --watch",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "lint:src": "eslint src/**/*.ts --fix",
    "start:prod": "nodemon src/index.ts"
  },
  "keywords": [
    "Microsoft",
    "Azure",
    "ms",
    "Microsoft Graph",
    "msg",
    "msgraph"
  ],
  "author": "Jackson Ming Hu <huming2207@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huming2207/TypeCal"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.7.11",
    "@types/omit-empty": "^1.0.0",
    "axios": "^0.19.0",
    "cerialize": "^0.1.18",
    "luxon": "^1.22.0",
    "moment": "^2.24.0",
    "omit-empty": "^1.0.0",
    "query-string": "^6.8.3",
    "ts-enum-util": "^4.0.1",
    "ts-jest": "^25.0.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@babel/polyfill": "7.8.3",
    "@types/luxon": "1.21.0",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "ts-node": "8.6.2"
  },
  "files": [
    "dist/**/*"
  ]
}
